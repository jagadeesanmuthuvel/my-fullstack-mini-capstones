<%- include("header")-%>

    <div class="box" id="heading">
  <h1>  <%=listTitle %></h1>
  </div>
  <div class="box">
    <form action="/delete" method="post">

    <% list.forEach(function(list){
      %>
      <div class="item">
        <input type="checkbox" name="checkbox" value=<%= list._id %> onChange="this.form.submit()">
      <p>
        <%= list.name %>
      </p></div>

  <%  }); %>
</form>

    <form class="item" action="/" method="post">
      <input type="text" name="item" value="" placeholder="New Item" autocomplete="off">
      <button type="submit" name="button" value="<%=listTitle %>">+</button>
    </form>
  </div>
  <%- include("footer")-%>
